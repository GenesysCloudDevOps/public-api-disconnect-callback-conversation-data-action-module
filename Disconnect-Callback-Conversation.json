{
  "name": "Disconnect Callback Conversation",
  "integrationType": "purecloud-data-actions",
  "actionType": "custom",
  "config": {
    "request": {
      "requestUrlTemplate": "/api/v2/conversations/callbacks/${input.ConversationId}",
      "requestType": "PATCH",
      "headers": {},
      "requestTemplate": "{ \"state\": \"disconnected\" }"
    },
    "response": {
      "translationMap": {},
      "translationMapDefaults": {},
      "successTemplate": "${rawResult}"
    }
  },
  "contract": {
    "input": {
      "inputSchema": {
        "type": "object",
        "properties": {
          "ConversationId": {
            "type": "string"
          }
        },
        "additionalProperties": true
      }
    },
    "output": {
      "successSchema": {
        "type": "object",
        "properties": {},
        "additionalProperties": true
      }
    }
  },
  "secure": false
}
